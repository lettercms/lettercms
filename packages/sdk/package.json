{
  "name": "@lettercms/sdk",
  "version": "0.0.14",
  "private": false,
  "main": "index.js",
  "engines": {
    "yarn": ">= 1.22",
    "node": ">=16"
  },
  "license": "Apache-2.0",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lettercms/lettercms.git"
  },
  "scripts": {
    "build": "tsc --outDir ./cjs -m commonjs -t ES2017",
    "lint": "echo done",
    "deploy:canary": "node ../../scripts/release-canary && npm publish --tag canary",
    "build-dist": "webpack",
    "build-cjs": "rimraf ./cjs && tsc --outDir ./cjs -m commonjs -t ES2017",
    "build-esm": "rimraf ./esm && tsc --outDir ./esm -m ESNext -t ESNEXT"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "isomorphic-fetch": "^3.0.0"
  }
}
